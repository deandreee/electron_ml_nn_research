{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "outDir": "dist-be",
    "baseUrl": ".",
    "target": "es6",
    "lib": ["dom", "es6"],
    "jsx": "react",
    "paths": {
      "*": ["node_modules/*"]
    },
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/**/*"],
  "files": ["./src/go.ts"]
}
